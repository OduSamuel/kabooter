  "name": "kabooter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^2.0.1",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "from": "^0.1.7",
    "import": "0.0.6",
    "jsonwebtoken": "^8.2.2",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^3.0.1",
    "koa-compose": "^4.1.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.1",
    "koa-static": "^4.0.3",
    "mustache": "^2.3.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.7",
    "nodemon": "^1.17.5",
    "socket.io": "^2.1.1",
    "upath": "^1.1.0",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "start-dev": "set DEBUG=Kabooter:*,knex:* && nodemon src/index.js --exec babel-node",
    "start-debug": "set DEBUG=Kabooter:* && npm start",
    "copy-files": "powershell -ExecutionPolicy ByPass -File copy-files-and-azure-configs.ps1",
    "delete-dist": "powershell -ExecutionPolicy ByPass -File delete-old-dist-files.ps1",
    "build": "npm run delete-dist && babel src -d dist --copy-files && npm run copy-files",
    "serve": "set DEBUG=Kabooter:* && node dist/index.js",
    "test": "mocha --require babel-register",
    "migrate-db": "knex migrate:latest --knexfile ./src/db/knexfile.js",
    "seed-db": "knex seed:run --knexfile ./src/db/knexfile.js",
    "load-db": "npm run migrate-db && npm run seed-db",
    "runapp": "npm run build "
  },
